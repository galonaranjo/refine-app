{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": false,
  "embeddedLanguageFormatting": "auto",
  "maxEmptyLines": 3,
  "preserveEmptyLines": true,
  "endOfLine": "auto",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "parser": "babel"
}
